{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "docs:build": "typedoc src/index.ts",
    "docs:start": "serve ./docs"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@rkingon/transunion-tuna-node",
  "author": "Roi Kingon",
  "repository": {
    "type": "git",
    "url": "https://github.com/rkingon/transunion-tuna-node"
  },
  "homepage": "https://rkingon.github.io/transunion-tuna-node",
  "module": "dist/transunion-tuna-node.esm.js",
  "size-limit": [
    {
      "path": "dist/transunion-tuna-node.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/transunion-tuna-node.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.3",
    "husky": "^7.0.4",
    "serve": "^13.0.2",
    "size-limit": "^7.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "fast-xml-parser": "^4.0.0-beta.6"
  }
}
